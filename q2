import pandas as pd
import numpy as np
df=pd.read_csv('C:\\Users\Julia\Desktop\python\gender_task_set_open.csv',sep=',')
df2=df.drop('id',axis=1)
df2.head()
from sklearn.cluster import KMeans
model = KMeans(n_clusters=2, random_state=35)
model.fit(df2)
model.cluster_centers_
df2['sex'] = model.labels_
df2.groupby('sex').mean()
df2
df2['sex']=df2['sex'].replace(0, 2)
df2.groupby('sex').count()
sex=pd.DataFrame(data=df2['sex'],index=df['id'])
sex['sex']
